{
    "env": {
        "es6": true,
        "node": true,
        "mocha": true
    },
    "extends": "airbnb-base",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "comma-dangle": "off",
        "no-underscore-dangle": "off",
        "no-unused-expressions": "off"
    }
}
